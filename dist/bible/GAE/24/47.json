    {
        "bcode": 24,
        "cnum": 47,
        "content": "\ubc14\ub85c\uac00 \uac00\uc0ac\ub97c \uce58\uae30 \uc804\uc5d0 \ube14\ub808\uc14b \uc0ac\ub78c\uc5d0 \ub300\ud558\uc5ec \uc120\uc9c0\uc790 \uc608\ub808\ubbf8\uc57c\uc5d0\uac8c \uc784\ud55c \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774\ub77c",
        "vcode": "GAE",
        "vnum": 1
    },
    {
        "bcode": 24,
        "cnum": 47,
        "content": "\uc5ec\ud638\uc640\uaed8\uc11c \uc774\uc640 \uac19\uc774 \ub9d0\uc500\ud558\uc2dc\ub418 \ubcf4\ub77c \ubb3c\uc774 \ubd81\ucabd\uc5d0\uc11c \uc77c\uc5b4\ub098 \ubb3c\uacb0\uce58\ub294 \uc2dc\ub0b4\ub97c \uc774\ub8e8\uc5b4 \uadf8 \ub545\uacfc \uadf8 \uc911\uc5d0 \uc788\ub294 \ubaa8\ub4e0 \uac83\uacfc \uadf8 \uc131\uc74d\uacfc \uac70\uae30\uc5d0 \uc0ac\ub294 \uc790\ub4e4\uc744 \ud729\uc4f8\ub9ac\ub2c8 \uc0ac\ub78c\ub4e4\uc774 \ubd80\ub974\uc9d6\uc73c\uba70 \uadf8 \ub545 \ubaa8\ub4e0 \uc8fc\ubbfc\uc774 \uc6b8\ubd80\uc9d6\uc73c\ub9ac\ub77c",
        "vcode": "GAE",
        "vnum": 2
    },
    {
        "bcode": 24,
        "cnum": 47,
        "content": "\uad70\ub9c8\uc758 \ubc1c\uad7d \uc18c\ub9ac\uc640 \ub2ec\ub9ac\ub294 \ubcd1\uac70 \ubc14\ud034\uac00 \uc9c4\ub3d9\ud558\ub294 \uc18c\ub9ac \ub54c\ubb38\uc5d0 \uc544\ubc84\uc9c0\uc758 \uc190\ub9e5\uc774 \ud480\ub824\uc11c \uc790\uae30\uc758 \uc790\ub140\ub97c \ub3cc\ubcf4\uc9c0 \ubabb\ud558\ub9ac\ub2c8",
        "vcode": "GAE",
        "vnum": 3
    },
    {
        "bcode": 24,
        "cnum": 47,
        "content": "\uc774\ub294 \ube14\ub808\uc14b \uc0ac\ub78c\uc744 \uc720\ub9b0\ud558\uc2dc\uba70 \ub450\ub85c\uc640 \uc2dc\ub3c8\uc5d0 \ub0a8\uc544 \uc788\ub294 \ubc14 \ub3c4\uc640 \uc904 \uc790\ub97c \ub2e4 \ub04a\uc5b4 \ubc84\ub9ac\uc2dc\ub294 \ub0a0\uc774 \uc62c \uac83\uc784\uc774\ub77c \uc5ec\ud638\uc640\uaed8\uc11c \uac11\ub3cc \uc12c\uc5d0 \ub0a8\uc544 \uc788\ub294 \ube14\ub808\uc14b \uc0ac\ub78c\uc744 \uc720\ub9b0\ud558\uc2dc\ub9ac\ub77c",
        "vcode": "GAE",
        "vnum": 4
    },
    {
        "bcode": 24,
        "cnum": 47,
        "content": "\uac00\uc0ac\ub294 \ub300\uba38\ub9ac\uac00 \ub418\uc5c8\uace0 \uc544\uc2a4\uae00\ub860\uacfc \uadf8\ub4e4\uc5d0\uac8c \ub0a8\uc544 \uc788\ub294 \ud3c9\uc9c0\uac00 \uc7a0\uc7a0\ud558\uac8c \ub418\uc5c8\ub098\ub2c8 \ub124\uac00 \ub124 \ubab8 \ubca0\uae30\ub97c \uc5b4\ub290 \ub54c\uae4c\uc9c0 \ud558\uaca0\ub290\ub0d0",
        "vcode": "GAE",
        "vnum": 5
    },
    {
        "bcode": 24,
        "cnum": 47,
        "content": "\uc624\ud638\ub77c \uc5ec\ud638\uc640\uc758 \uce7c\uc774\uc5ec \ub124\uac00 \uc5b8\uc81c\uae4c\uc9c0 \uc26c\uc9c0 \uc54a\uaca0\ub290\ub0d0 \ub124 \uce7c\uc9d1\uc5d0 \ub4e4\uc5b4\uac00\uc11c \uac00\ub9cc\ud788 \uc274\uc9c0\uc5b4\ub2e4",
        "vcode": "GAE",
        "vnum": 6
    },
    {
        "bcode": 24,
        "cnum": 47,
        "content": "\uc5ec\ud638\uc640\uaed8\uc11c \uc774\ub97c \uba85\ub839\ud558\uc168\uc740\uc989 \uc5b4\ub5bb\uac8c \uc7a0\uc7a0\ud558\uba70 \uc26c\uaca0\ub290\ub0d0 \uc544\uc2a4\uae00\ub860\uacfc \ud574\ubcc0\uc744 \uce58\ub824 \ud558\uc5ec \uadf8\uac00 \uc815\ud558\uc168\ub290\ub2c8\ub77c \ud558\ub2c8\ub77c",
        "vcode": "GAE",
        "vnum": 7
    }
]